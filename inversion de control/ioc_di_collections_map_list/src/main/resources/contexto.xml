<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


	<bean id="col1" class="es.curso.spring.Coleccion">
		<property name="datos">
			<list>
				<value>Datos1 list</value>
				<value>Datos2 list</value>
			</list>		
		</property>
	</bean>
	
	<bean id="col2" class="es.curso.spring.Coleccion">
		<property name="datos">
			<set>
				<value>Datos1 set</value>
				<value>Datos2 set</value>
			</set>		
		</property>
	</bean>
</beans>
